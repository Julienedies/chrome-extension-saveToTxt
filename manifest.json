{
  "manifest_version": 2,
  "name": "save2txt",
  "description": "Save selected text to TXT file.",
  "version": "1.1.2",
  "icons": {
    "16": "src/img/icon-bitty.png",
    "48": "src/img/icon-small.png",
    "128": "src/img/icon-large.png"
  },
  "browser_action": {
    "default_icon": "src/img/icon-large.png",
    "default_popup": "src/html/popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "storage",
    "\u003Call_urls\u003E",
    "http://*/*",
    "https://*/*"
  ]
}
